  "jobs": {
    "zadost_po_minute": {
      "type": "workflow",
      "cron": "0 * * * * ?"
    },
    
    "notifikace_den": {
      "type": "notifications",
      "interval": "den",
      "cron": "3 0 0 * * ?"
    },
    "notifikace_tyden": {
      "type": "notifications",
      "interval": "tyden",
      "cron": "3 0 2 ? * MON"
    },
    "notifikace_mesic": {
